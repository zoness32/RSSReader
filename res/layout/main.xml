<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<LinearLayout android:id="@+id/linearLayout1"
		android:layout_width="fill_parent" android:layout_height="wrap_content">
		<Button android:id="@+id/btnLoad" android:onClick="btnLoad_clicked"
			android:layout_width="wrap_content" android:text="Load"
			android:layout_height="wrap_content"></Button>
		<EditText android:id="@+id/txtURL" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="http://csunix.cs.bju.edu/~aeern996/test.rss"
			android:maxLines="1"></EditText>
	</LinearLayout>
	<ListView android:layout_height="wrap_content"
		android:layout_width="fill_parent" android:id="@android:id/list"></ListView>
</LinearLayout>
